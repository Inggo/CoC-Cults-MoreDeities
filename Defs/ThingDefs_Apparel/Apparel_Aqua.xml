<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>MoreDeities_Aqua_ApparelRobe</defName>
    <description>This sacred wrap allows priests to preach with greater appeal by appealing to the Goddess Aqua. Fits the outer layer.</description>
    <label>cultist robes (Aqua)</label>
    <graphicData>
      <texPath>Apparel/Bodywear_Clothing/NyarlathotepRobes/ROMNyarlathotepRobes</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <generateCommonality>0.01</generateCommonality>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>80</costStuffCount>
    <statBases>
      <Mass>0.15</Mass>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToMake>8000</WorkToMake>
      <StuffEffectMultiplierArmor>0.3</StuffEffectMultiplierArmor>
		  <StuffEffectMultiplierInsulation_Cold>0.6</StuffEffectMultiplierInsulation_Cold>
		  <StuffEffectMultiplierInsulation_Heat>0.6</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.3</SocialImpact>
      <ResearchSpeed>0.1</ResearchSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Bodywear_Clothing/NyarlathotepRobes/ROMNyarlathotepRobes</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
        <li>CallOfCthulhu_FrenziedCultist</li>
      </tags>
      <defaultOutfitTags>
        <li>CallOfCthulhu_FrenziedCultist</li>
      </defaultOutfitTags>
    </apparel>
    <recipeMaker>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
        <li>CraftingSpot</li>
      </recipeUsers>
      <researchPrerequisite>Forbidden_Attire</researchPrerequisite>
    </recipeMaker>
    <comps>
      <li Class="CultOfCthulhu.CompProperties_FavoredObject">
        <deities>
          <li>
            <deityDef>Entity_Aqua</deityDef>
            <favorBonus>0.1</favorBonus>
          </li>
        </deities>
      </li>
    </comps>
    <colorGenerator Class="ColorGenerator_Single">
      <color>RGBA(1.0,1.0,1.0,1.0)</color>
    </colorGenerator>
  </ThingDef>

